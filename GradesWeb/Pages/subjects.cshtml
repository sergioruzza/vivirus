@page

@{
  Layout = "Shared/_MenuLayout.cshtml";
}

@model GradesWeb.Pages.SubjectsModel
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers

<div class="row">
  <h1 class="display-2">Subjects</h1>
  <table class="table">
    <thead class="thead-inverse">
      <tr><th>School Subjects</th></tr>
    </thead>
    <tbody>
    @foreach(var subject in Model.Subjects)
    {
      <tr><td>@subject.Name</td></tr>
    }
    </tbody>
  </table>
</div>
<div class="row">
</div>